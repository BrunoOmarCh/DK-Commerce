@{
    ViewBag.Title = "CompaniaDeEnvio - Editar";
}
@section Styles {
    <link href="~/views/companiaDeEnvio/editar.css" rel="stylesheet" />
}
<form id="CompaniaDeEnvioEditarFrm">
    <!-- Botón para guardar -->
    <button id="GuardarBtn" type="button">Guardar</button>
    <br /><br />

    <!-- Campos del formulario -->
    <label for="CompaniaDeEnvioIdTxt">ID Compañía</label>
    <input id="CompaniaDeEnvioIdTxt" type="text" value="@ViewBag.CompaniaDeEnvioId" readonly="readonly" />
    <br />

    <label for="NombreCompañiaTxt">Nombre de la Compañía</label>
    <input id="NombreCompañiaTxt" type="text" value="@ViewBag.NombreCompañia" />
    <br />

    <label for="RucTxt">RUC</label>
    <input id="RucTxt" type="text" value="@ViewBag.Ruc" />
    <br />

    <label for="TelefonoTxt">Teléfono</label>
    <input id="TelefonoTxt" type="text" value="@ViewBag.Telefono" />
    <br />
</form>

@section Scripts {
    <script src="~/views/companiaDeEnvio/editar.js"></script>
}