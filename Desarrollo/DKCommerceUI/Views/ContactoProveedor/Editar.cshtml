@{
    ViewBag.Title = "ContactoProveedor - Editar";
}
@section Styles {
    <link href="~/views/contactoProveedor/editar.css" rel="stylesheet" />
}
<form id="ContactoProveedorEditarFrm">
    <!-- Botón para guardar -->
    <button id="GuardarBtn" type="button">Guardar</button>
    <br /><br />

    <!-- Campos del formulario -->
    <label for="ContactoProveedorIdTxt">ID Contacto Proveedor</label>
    <input id="ContactoProveedorIdTxt" type="text" value="@ViewBag.ContactoProveedorId" readonly="readonly" />
    <br />

    <label for="NombreContactoTxt">Nombre del Contacto</label>
    <input id="NombreContactoTxt" type="text" value="@ViewBag.NombreContacto" />
    <br />

    <label for="CargoContactoTxt">Cargo del Contacto</label>
    <input id="CargoContactoTxt" type="text" value="@ViewBag.CargoContacto" />
    <br />

    <label for="DniTxt">DNI</label>
    <input id="DniTxt" type="text" value="@ViewBag.Dni" />
    <br />
</form>
@section Scripts {
    <script src="~/views/contactoProveedor/editar.js"></script>
}

