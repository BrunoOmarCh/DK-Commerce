@{
    ViewBag.Title = "Empleado - Nuevo";
}
@section Styles {
    <link href="~/views/empleado/nuevo.css" rel="stylesheet" />
}

<form id="EmpleadoNuevoFrm">
    <button id="GuardarBtn" type="button">Guardar</button>
    <br /><br />

    <label for="NombresTxt">Nombres</label>
    <input id="NombresTxt" type="text" autocomplete="off" />
    <br />

    <label for="ApellidoPaternoTxt">Apellido Paterno</label>
    <input id="ApellidoPaternoTxt" type="text" autocomplete="off" />
    <br />

    <label for="ApellidoMaternoTxt">Apellido Materno</label>
    <input id="ApellidoMaternoTxt" type="text" autocomplete="off" />
    <br />

    <label for="TipoDocTxt">Tipo Documento</label>
    <input id="TipoDocTxt" type="text" autocomplete="off" />
    <br />

    <label for="NroDocTxt">Nro Documento</label>
    <input id="NroDocTxt" type="text" autocomplete="off" />
    <br />

    <label for="CargoTxt">Cargo</label>
    <input id="CargoTxt" type="text" autocomplete="off" />
    <br />

    <label for="EmailTxt">Email</label>
    <input id="EmailTxt" type="email" autocomplete="off" />
    <br />
</form>

@section Scripts {
    <script src="~/views/empleado/nuevo.js"></script>
}
