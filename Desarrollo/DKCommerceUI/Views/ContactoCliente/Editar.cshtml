@{
    ViewBag.Title = "ContactoCliente - Editar";
}
@section Styles {
    <link href="~/views/contactoCliente/editar.css" rel="stylesheet" />
}
<form id="ContactClienteoEditarFrm">
    <!-- Botón para guardar -->
    <button id="GuardarBtn" type="button">Guardar</button>
    <br /><br />

    <!-- Campos del formulario -->
    <label for="ContactoIdTxt">ID Contacto</label>
    <input id="ContactoIdTxt" type="text" value="@ViewBag.ContactoId" readonly="readonly" />
    <br />

    <label for="NombreContactoTxt">Nombre del Contacto</label>
    <input id="NombreContactoTxt" type="text" value="@ViewBag.NombreContacto" />
    <br />

    <label for="CargoContactoTxt">Cargo del Contacto</label>
    <input id="CargoContactoTxt" type="text" value="@ViewBag.CargoContacto" />
    <br />

    <label for="TipoDocumentoTxt">Tipo de Documento</label>
    <input id="TipoDocumentoTxt" type="text" value="@ViewBag.TipoDocumento" />
    <br />

    <label for="NroDocumentoTxt">Número de Documento</label>
    <input id="NroDocumentoTxt" type="text" value="@ViewBag.NroDocumento" />
    <br />
</form>
@section Scripts {
    <script src="~/views/contactoCliente/editar.js"></script>
}

